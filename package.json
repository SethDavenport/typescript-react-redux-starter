{
  "name": "typescript-react-redux-starter",
  "version": "0.1.0",
  "description": "A boiler-plate setup for TypeScript/React/Redux applications",
  "license": "MIT",
  "engines": {
    "node": "6"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p",
    "clean": "rimraf dist/",
    "dev:server": "cross-env NODE_ENV=development PORT=3000 nodemon server/node-server.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info",
    "dev": "concurrently -r \"npm run dev:client\" \"npm run dev:server\"",
    "lint": "npm run lint-ts && npm run lint-js && npm run lint-css",
    "lint-css": "stylelint \"./src/**/*.css\"",
    "lint-ts": "tslint \"src/**/*.tsx\" \"src/**/*.ts\" \"test/**/*.tsx\" \"test/**/*.ts\"",
    "lint-js": "eslint \"**/*.js\" --ignore-path .gitignore",
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production node server/node-server.js",
    "pretest": "npm run lint",
    "test": "karma start --singleRun true",
    "test:watch": "karma start --singleRun false",
    "posttest": "istanbul check-coverage --statement 75 --branch 50 --function 50"
  },
  "keywords": [
    "react",
    "redux",
    "typescript",
    "starter",
    "seed"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rangle/typescript-react-redux-starter.git"
  },
  "bugs": {
    "url": "https://github.com/rangle/rangle-starter/issues"
  },
  "dependencies": {
    "@types/classnames": "^0.0.32",
    "@types/core-js": "^0.9.35",
    "@types/enzyme": "^2.7.1",
    "@types/jasmine": "^2.5.40",
    "@types/node": "^6.0.58",
    "@types/react-dom": "^0.14.20",
    "@types/react-router": "^2.0.41",
    "@types/react-router-redux": "^4.0.36",
    "@types/whatwg-fetch": "^0.0.33",
    "body-parser": "1.15.2",
    "express": "4.14.0",
    "helmet": "3.3.0",
    "http-proxy": "1.16.0",
    "winston": "2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "6.6.1",
    "awesome-typescript-loader": "3.0.0-beta.17",
    "basscss": "8.0.3",
    "basscss-addons": "1.0.0",
    "basscss-basic": "1.0.0",
    "classnames": "2.2.5",
    "concurrently": "3.1.0",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "cssnano": "3.10.0",
    "denodeify": "1.2.1",
    "enzyme": "2.7.0",
    "eslint": "3.12.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "history": "4.5.0",
    "html-webpack-plugin": "2.26.0",
    "immutable": "3.8.1",
    "imports-loader": "0.7.0",
    "istanbul": "^1.0.0-alpha.2",
    "istanbul-instrumenter-loader": "^1.0.0",
    "jasmine-core": "2.4.1",
    "json-loader": "0.5.4",
    "karma": "1.1.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.0.0",
    "karma-jasmine": "1.0.2",
    "karma-remap-istanbul": "0.1.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-sourcemap-writer": "0.1.2",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "1.7.0",
    "nodemon": "1.11.0",
    "object-assign": "4.1.0",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "postcss-cssnext": "2.9.0",
    "postcss-import": "9.0.0",
    "postcss-loader": "1.2.1",
    "postcss-modules-local-by-default": "1.1.1",
    "raw-loader": "0.5.1",
    "react": "15.4.1",
    "react-addons-test-utils": "15.4.1",
    "react-dom": "15.4.1",
    "react-redux": "5.0.1",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.7",
    "redux": "3.6.0",
    "redux-form": "5.3.3",
    "redux-localstorage": "0.4.1",
    "redux-logger": "2.7.4",
    "redux-thunk": "2.1.0",
    "require-hacker": "2.1.4",
    "rimraf": "2.5.4",
    "source-map-support": "0.4.8",
    "style-loader": "0.13.1",
    "stylelint": "7.7.1",
    "stylelint-webpack-plugin": "0.4.2",
    "ts-helpers": "1.1.2",
    "tslint": "4.3.1",
    "tslint-loader": "3.3.0",
    "typescript": "2.1.4",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-hot-middleware": "2.15.0",
    "whatwg-fetch": "2.0.1"
  }
}
